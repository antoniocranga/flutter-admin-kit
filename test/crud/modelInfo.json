{
  "name": "Event",
  "name_plural": "Events",
  "fields": {
    "id": {
      "type": "integer",
      "required": false,
      "read_only": true,
      "label": "ID"
    },
    "timezone": {
      "type": "string",
      "required": true,
      "read_only": false,
      "label": "Timezone"
    },
    "date": {
      "type": "datetime",
      "required": false,
      "read_only": false,
      "label": "Date"
    },
    "duration": {
      "type": "duration",
      "required": false,
      "read_only": false,
      "label": "Duration"
    },
    "name": {
      "type": "string",
      "required": true,
      "read_only": false,
      "label": "Name",
      "max_length": 255
    },
    "teaser": {
      "type": "string",
      "required": false,
      "read_only": false,
      "label": "Teaser"
    },
    "slug": {
      "type": "slug",
      "required": true,
      "read_only": false,
      "label": "Slug",
      "max_length": 50
    },
    "language_code": {
      "type": "string",
      "required": false,
      "read_only": false,
      "label": "Language code",
      "max_length": 2
    },
    "type": {
      "type": "choice",
      "required": false,
      "read_only": false,
      "label": "Type",
      "choices": [
        {
          "value": "DANCE",
          "display_name": "DANCE"
        },
        {
          "value": "COUPLE_DANCE",
          "display_name": "COUPLE_DANCE"
        },
        {
          "value": "WORKSHOP",
          "display_name": "WORKSHOP"
        },
        {
          "value": "ONLINE",
          "display_name": "ONLINE"
        }
      ]
    },
    "state": {
      "type": "choice",
      "required": false,
      "read_only": false,
      "label": "State",
      "choices": [
        {
          "value": "DRAFT",
          "display_name": "DRAFT"
        },
        {
          "value": "PUBLISHED",
          "display_name": "PUBLISHED"
        },
        {
          "value": "ARCHIVED",
          "display_name": "ARCHIVED"
        }
      ]
    },
    "listed": {
      "type": "boolean",
      "required": false,
      "read_only": false,
      "label": "Listed",
      "help_text": "Unchecking makes the event not appear in the event index and only accessible by a direct link."
    },
    "highlighted": {
      "type": "boolean",
      "required": false,
      "read_only": false,
      "label": "Highlighted",
      "help_text": "Highlights the event and shows them preferably as teaser event of series, also when archived."
    },
    "active": {
      "type": "boolean",
      "required": false,
      "read_only": false,
      "label": "Active",
      "help_text": "Defines if clients can subscribe to this event."
    },
    "created_at": {
      "type": "datetime",
      "required": false,
      "read_only": true,
      "label": "Created at"
    },
    "modified_at": {
      "type": "datetime",
      "required": false,
      "read_only": true,
      "label": "Modified at"
    },
    "price_min": {
      "type": "float",
      "required": false,
      "read_only": false,
      "label": "Price min",
      "help_text": "The minimum price. If no maximum price is set, this is the fixed price."
    },
    "price_max": {
      "type": "float",
      "required": false,
      "read_only": false,
      "label": "Price max",
      "help_text": "The maximum price. Requires minimum price to be set."
    },
    "meta_description": {
      "type": "string",
      "required": false,
      "read_only": false,
      "label": "Description",
      "help_text": "The text displayed in search engines.",
      "max_length": 155
    },
    "eventbrite_id": {
      "type": "string",
      "required": false,
      "read_only": false,
      "label": "Eventbrite id",
      "help_text": "The id of eventbrite's event to inline checkout.",
      "max_length": 50
    },
    "series": {
      "type": "field",
      "required": true,
      "read_only": false,
      "label": "Series"
    },
    "site": {
      "type": "field",
      "required": false,
      "read_only": false,
      "label": "Site",
      "help_text": "Only show on a specific site. Leave empty to show on all sites."
    },
    "description": {
      "type": "field",
      "required": false,
      "read_only": true,
      "label": "Description"
    },
    "cover": {
      "type": "field",
      "required": false,
      "read_only": false,
      "label": "Cover"
    },
    "location": {
      "type": "field",
      "required": false,
      "read_only": false,
      "label": "Location"
    }
  },
  "list_display": [
    "name",
    "slug",
    "series",
    "date",
    "language_code",
    "state",
    "site",
    "highlighted",
    "active",
    "tag_list"
  ],
  "ordering_fields": [
    "name",
    "slug",
    "series",
    "date",
    "language_code",
    "state",
    "site",
    "highlighted",
    "active",
    "tag_list"
  ],
  "filterset_fields": [
    "site",
    "state",
    "series",
    "date"
  ]
}